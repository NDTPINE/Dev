@page "/pages/results"

<div class=" m-2 ">
    <table class="table table-sm table-bordered table-striped">
        <thead>
            <tr class="bg-primary text-white text-center ">
                <th colspan="2">Form Data</th>
            </tr>
        </thead>
        <tbody>
            @foreach(string key in TempData.Keys)
            {
                <tr>
                    <th>@key</th>
                    <td>@TempData[key]</td>
                </tr>
            }
        </tbody>
    </table>
</div>