@page
@inject DataContext context;

<h5 class="bg-primary text-center text-white p-2 m-2">Categories</h5>
<ul class="list-group m-2">
    @foreach (Category category in context.Categories)
    {
        <li class="list-group-item">@category.Name</li>
    }
</ul>
<div class="bg-primary text-white text-center p-2 m-2"><vc:cities-controller-hybrid></vc:cities-controller-hybrid></div>


