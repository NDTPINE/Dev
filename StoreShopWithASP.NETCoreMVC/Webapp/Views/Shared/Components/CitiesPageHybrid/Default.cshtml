@model CityViewModel

<table class="table table-striped table-sm table-bordered text-white text-center ">
    <thead><tr><th colspan="2">Hybrid Page Summary</th></tr></thead>
    <tbody>
        <tr>
            <td>Cities:</td>
            <td class=" text-right">@Model.Cities</td>
        </tr>
        <tr>
            <td>Population</td>
            <td class=" text-right">@Model.Population.ToString("#,###")</td>
        </tr>
    </tbody>
</table>